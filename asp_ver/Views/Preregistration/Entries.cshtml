@model List<Entry>

<table class="table">
    <thead>
        <tr>
            <th>#</th>
            <th>Имя</th>
            <th>Дата и время</th>
            <th>Телефон</th>
            <th>Дополнительно</th> 
        </tr>
    </thead>
    <tbody>
        @foreach(var e in Model){
        <form method="post">
            <tr class="table-light">
            <td>@e.Id</td>
            <td><input type="text" value=@e.Owner/></td>
            <td>@e.Date</td>
            <td>@e.Phone</td>
            <td>@e.Description</td>
            </tr>
        </form>
        }
    </tbody>
</table>    


